{
  "name": "rsk-contract-verifier",
  "version": "0.0.2",
  "description": "Smart contract source code verifier",
  "main": "",
  "scripts": {
    "dev": "npx nodemon src/contract-verifier-api.js | npx bunyan -o short",
    "build": "npm run clean && npx babel src -d dist --copy-files",
    "clean": "rm -rf dist",
    "md": "mkdir dist",
    "lint": "npx eslint src/**/*",
    "test": "npx mocha",
    "test-file": "npx mocha --opts ./test/singleFile.opts",
    "cache-solc": "node dist/tools/cacheSolc.js | npx bunyan "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rsksmart/rsk-contract-verifier"
  },
  "keywords": [
    "rsk",
    "verifier"
  ],
  "author": "emi",
  "license": "MIT",
  "homepage": "https://github.com/rsksmart/rsk-contract-verifier",
  "dependencies": {
    "axios": "^0.19.0",
    "bunyan": "^2.0.2",
    "cbor": "^4.1.5",
    "ethereumjs-account": "^3.0.0",
    "ethereumjs-tx": "^2.1.0",
    "ethereumjs-util": "^6.1.0",
    "ethereumjs-vm": "^4.0.0-beta.1",
    "require-from-string": "^2.0.2",
    "socket.io": "^2.2.0",
    "solc": "^0.5.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-bytes": "^0.1.2",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "glob": "^7.1.4",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "openzeppelin-solidity": "^2.3.0"
  }
}
